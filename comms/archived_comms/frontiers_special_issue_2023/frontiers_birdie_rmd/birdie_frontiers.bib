	pages = {157--175},
	file = {Buckland et al. - 2004 - State-space models for the dynamics of wild animal.pdf:C\:\\Users\\P.Cervantes\\Zotero\\storage\\EE4JTZDZ\\Buckland et al. - 2004 - State-space models for the dynamics of wild animal.pdf:application/pdf},
